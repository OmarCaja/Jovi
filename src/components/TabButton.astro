---
import "../styles/components/TabButton.less";
import { TabButtonItem } from "../types/tabButtonItem";

interface Props {
	tabButton: TabButtonItem;
}

const { tabButton } = Astro.props;
---

<script src="../scripts/components/tabButton.ts"></script>

<button
	role="button"
	class="tab-button js-tab-button"
	data-target-id={tabButton.id}
>
	{tabButton.title}
</button>
